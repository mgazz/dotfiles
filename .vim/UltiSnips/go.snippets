snippet printmap "print map" b
fmt.Println("Printing $1")
for key, value := range ${1:map}{
  fmt.Println("[----] Key:", key, "Value:", value)
}
endsnippet

snippet httpresp "print HTTP response" b
buf := new(bytes.Buffer)
buf.ReadFrom(responseBody)
b := buf.Bytes()
s := *(*string)(unsafe.Pointer(&b))
fmt.Println("[--] Response" + s)
endsnippet

snippet dbgmessage "debug message using fmt.Println" b
fmt.Println("[----] ${1:message}")
endsnippet
